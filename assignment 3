w = ["level", "noon", "goal", "radar", "bed", "soccer", "civic", "hotel", "cake", "madam"]
for x in w:
  palindrome = True
  length = len(x)
  i = 0
  while i < length // 2:
    if x[i] != x[length - 1 - i]:
      palindrome = False
    i += 1
  if palindrome:
    print(x , "is a palindrome")
  else:
    print(x , "is not a palindrome")
